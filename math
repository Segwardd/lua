local library = {}

function library:sigmoid(x)
    return 1 / (1 + math.exp(-x))
end

function library:sigmoid_prime(x)
    return library:sigmoid(x) * (1 - library:sigmoid(x))
end

function library:square(x)
    return x * x
end

function library:sigma(input,weight,bias)
    total = 0
    for i = 1, #input do
        total = total + input[i] * weight[i]
    end
    return total + bias
end

return library
